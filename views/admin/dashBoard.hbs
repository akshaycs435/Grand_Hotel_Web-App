                <div class="col-xl-4 col-lg-12">
                    <div class="card mb-4">
                        <article class="card-body">
                            <h5 class="card-title">Marketing Chanel</h5>
                            <span class="text-muted font-xs">customers</span>
                            <div class="progress mb-3">
                                <div class="progress-bar" role="progressbar" style="width: {{this.customerscount}}%">{{this.customerscount}}%</div>
                            </div>
                            <span class="text-muted font-xs">Rooms</span>
                            <div class="progress mb-3">
                                <div class="progress-bar" role="progressbar" style="width: {{this.roomsCount}}%">{{this.roomsCount}}%</div>
                            </div>
                            <span class="text-muted font-xs">Hotels</span>
                            <div class="progress mb-3">
                                <div class="progress-bar" role="progressbar" style="width: {{this.hotels}}%"> {{this.hotels}}%</div>
                            </div>
                            <span class="text-muted font-xs">facilities</span>
                            <div class="progress mb-3">
                                <div class="progress-bar" role="progressbar" style="width: {{this.facilitiescount}}%"> {{this.facilitiescount}}%</div>
                            </div>
                            <span class="text-muted font-xs">payment</span>
                            <div class="progress mb-3">
                                <div class="progress-bar" role="progressbar" style="width: {{this.paymentcount}}%"> {{this.paymentcount}}%</div>
                            </div>
                        </article>
                    </div>
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <h4 class="card-title">paid users</h4>
                   
                </header>
                <div class="card-body">
                    <div class="table-responsive">
                        <div class="table-responsive">
                            <table class="table align-middle table-nowrap mb-0">
                                <thead class="table-light">
                                    <tr>
                                        <th class="align-middle" scope="col">HotelId</th>
                                        <th class="align-middle" scope="col">RoomId</th>
                                        <th class="align-middle" scope="col">Room Type</th>
                                        <th class="align-middle" scope="col">UserId</th>
                                        <th class="align-middle" scope="col">Amount</th>
                                        <th class="align-middle" scope="col">Date</th>
                                        <th class="align-middle" scope="col">Action</th>
                                    </tr>
                                </thead>
               <tbody>

                                    {{#each transactiondata}}

                                    <tr>
                                        <td>
                                        <span class="badge badge-pill badge-soft-success">{{this.hotelId}}</span>
                                        </td>
                                        <td>
                                            <span class="badge badge-pill badge-soft-danger">{{this.roomId}}</span>
                                        </td>
                                                                                <td>
                                            {{this.name}}
                                        </td>
                                        <td>
                                            {{this.userId}}
                                        </td>
                                        
                                        <td>
                                            â‚¹{{this.amount}}
                                        </td>
                                       <td>
                                            {{this.payment_date}}
                                        </td>
                                        <td>
                                            <i class="material-icons md-payment font-xxl text-muted mr-5"></i>{{this.status}}
                                        </td>
                                    </tr>

                                    {{/each}}
                                    
                                </tbody>


                            </table>
                        </div>
                    </div> <!-- table-responsive end// -->
                </div>
            </div>
